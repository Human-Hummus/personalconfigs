

call plug#begin()
Plug 'itchyny/lightline.vim'
Plug 'prettier/vim-prettier'
call plug#end()

set number
set mouse=a
set clipboard=unnamedplus
set ignorecase
set nosmartcase
set completeopt=menuone,noselect
set hlsearch
set autoindent
set autoread
set belloff=all
set cursorline
set nowrap
set tw=99999
set noet
set nolinebreak
set tabstop=4
set shiftwidth=4
set noexpandtab
set noet ci pi sts=0 sw=4 ts=4
